        "def factorial(n):\n",
        "  if n == 0:\n",
        "    return 1\n",
        "  else:\n",
        "    return n * factorial(n-1)\n",
        "\n",
        "num = int(input(\"enter a number: \"))\n",
        "result = factorial(num)\n",
        "print(f\"the factorial of {num} is {result}\")"
      ],
